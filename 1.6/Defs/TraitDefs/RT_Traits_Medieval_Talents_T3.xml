          <IncomingDamageFactor>-0.35</IncomingDamageFactor>
          <CarryingCapacity>5</CarryingCapacity>
          <RestRateMultiplier>0.15</RestRateMultiplier>
          <MoveSpeed>0.2</MoveSpeed>
          <PawnTrapSpringChance>-0.1</PawnTrapSpringChance>
          <MentalBreakThreshold>-0.15</MentalBreakThreshold>
          <PainShockThreshold>0.15</PainShockThreshold>
          <ComfyTemperatureMax>5</ComfyTemperatureMax>
          <ComfyTemperatureMin>-5</ComfyTemperatureMin>
          <CleaningSpeed>0.05</CleaningSpeed>
        </statOffsets>
        <statFactors>
          <RTMT_FoodNeedDecay>0.90</RTMT_FoodNeedDecay>
  		    <RTMT_RestNeed_Decay>0.90</RTMT_RestNeed_Decay>
  		    <RTMT_RecreationNeed_Decay>0.90</RTMT_RecreationNeed_Decay>
        </statFactors>
        <skillGains>
